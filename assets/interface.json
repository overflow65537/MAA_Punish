            "option": [
                "选择温度",
                "选择效果"
            ]
        },
        {
            "name": "单人自动战斗测试(囚笼版待改)",
            "entry": "单人自动战斗",
            "pipeline_override": {
                "自动战斗开始": {
                    "post_delay": 2000
                }
            }
        ],
        "option": {
            "是否使用体力": {
                "cases": [
                    {
                        "name": "是",
                        "pipeline_override": {
                            "A_不使用体力": {
