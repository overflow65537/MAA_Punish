{
    "name": "MPA",
    "mirrorchyan_rid": "MAA_Punish",
    "url": "https://github.com/overflow65537/MAA_Punish",
    "custom_title": "MFAWPF_MPA",
    "default_controller": "adb",
    "lock_controller": true,
    "controller": [
        {
            "name": "安卓端",
            "type": "Adb"
        }
    ],
    "resource": [
        {
            "name": "官服",
            "path": [
                "{PROJECT_DIR}/resource/base"
            ]
        },
        {
            "name": "B 服",
            "path": [
                "{PROJECT_DIR}/resource/base",
                "{PROJECT_DIR}/resource/bilibili"
            ]
        }
    ],
    "task": [
        {
            "name": "进入游戏",
            "entry": "启动"
        },
        {
            "name": "指挥局",
            "entry": "指挥局"
        },
        {
            "name": "纷争战区",
            "entry": "纷争战区"
        },
        {
            "name": "诺曼复兴战",
            "entry": "诺曼复兴战"
        },
        {
            "name": "幻痛囚笼",
            "entry": "幻痛囚笼",
            "pipeline_override": {
                "战斗中": {
                    "post_delay": 1000
                }
            }
        },
        {
            "name": "领取体力",
            "entry": "领取体力"
        },
        {
            "name": "领取邮件",
            "entry": "邮件"
        },
        {
            "name": "购买碎片",
            "entry": "购买碎片"
        },
        {
            "name": "宿舍委托",
            "entry": "宿舍委托"
        },
        {
            "name": "宿舍任务",
            "entry": "宿舍任务"
        },
        {
            "name": "拟战场域",
            "entry": "拟战场域",
            "option": [
                "使用临期体力"
            ]
        },
        {
            "name": "守护者行动",
            "entry": "维系者行动"
        },
        {
            "name": "领取任务",
            "entry": "任务"
        },
        {
            "name": "战令",
            "entry": "通行证"
        },
        {
            "name": "结束游戏",
            "entry": "停止"
        },
        {
            "name": "寒境曙光",
            "entry": "寒境曙光",
            "option": [
                "选择温度",
                "选择效果"
            ]
        }
    ],
    "option": {
        "选择温度": {
            "cases": [
                {
                    "name": "常温",
                    "pipeline_override": {
                        "选择具体难度_寒境曙光": {
                            "expected": "^常温$"
                        }
                    }
                },
                {
                    "name": "初冷",
                    "pipeline_override": {
                        "选择具体难度_寒境曙光": {
                            "expected": "^初冷$"
                        }
                    }
                },
                {
                    "name": "微寒",
                    "pipeline_override": {
                        "选择具体难度_寒境曙光": {
                            "expected": "^微寒$"
                        }
                    }
                },
                {
                    "name": "冰冽",
                    "pipeline_override": {
                        "选择具体难度_寒境曙光": {
                            "expected": "^冰冽$"
                        }
                    }
                },
                {
                    "name": "严霜",
                    "pipeline_override": {
                        "选择具体难度_寒境曙光": {
                            "expected": "^严霜$"
                        }
                    }
                },
                {
                    "name": "极寒",
                    "pipeline_override": {
                        "选择具体难度_寒境曙光": {
                            "expected": "^极寒$"
                        }
                    }
                }
            ]
        },
        "选择效果": {
            "cases": [
                {
                    "name": "军事",
                    "pipeline_override": {
                        "选择具体开局_寒境曙光": {
                            "expected": "^军事$"
                        }
                    }
                },
                {
                    "name": "基建",
                    "pipeline_override": {
                        "选择具体开局_寒境曙光": {
                            "expected": "^基建$"
                        }
                    }
                },
                {
                    "name": "经济",
                    "pipeline_override": {
                        "选择具体开局_寒境曙光": {
                            "expected": "^经济$"
                        }
                    }
                },
                {
                    "name": "启动资金",
                    "pipeline_override": {
                        "选择具体开局_寒境曙光": {
                            "expected": "^启动资金$"
                        }
                    }
                }
            ]
        },
        "使用临期体力": {
            "cases": [
                {
                    "name": "是",
                    "pipeline_override": {
                        "结束战斗任务_不使用体力": {
                            "enabled": false
                        }
                    }
                },
                {
                    "name": "否",
                    "pipeline_override": {
                        "补给包不足_拟战场域": {
                            "enabled": false
                        },
                        "暂无血清组可使用": {
                            "enabled": false
                        },
                        "使用体力界面": {
                            "enabled": false
                        },
                        "结束战斗任务_使用体力": {
                            "enabled": false
                        }
                    }
                }
            ]
        }
    }
}