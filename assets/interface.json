        {
            "name": "安卓端",
            "type": "Adb"
        }
    ],
    "resource": [
        {
            "name": "官服",
            "path": [
                "{PROJECT_DIR}/resource/base"
            ]
        },
        {
            "name": "B 服",
            "path": [
                "{PROJECT_DIR}/resource/base",
                "{PROJECT_DIR}/resource/bilibili"
            ]
        }
    ],
    "task": [
        {
            "name": "进入游戏",
            "entry": "启动"
        },
        {
            "name": "指挥局",
            "entry": "指挥局"
        },
        {
            "name": "纷争战区",
            "entry": "纷争战区"
        },
        {
            "name": "诺曼复兴战",
            "entry": "诺曼复兴战"
        },
        {
            "name": "幻痛囚笼",
            "entry": "幻痛囚笼",
            "pipeline_override": {
                "战斗中": {
                    "post_delay": 1000
                }
            }
        },
        {
            "name": "刷A级碎片",
            "entry": "刷A级碎片",
            "pipeline_override": {
                "检查主界面": {
                    "action": "Click",
                    "next": [
                        "进入资源副本"
                    ]
                }
            },
            "option": [
                "选择A级角色",
                "是否使用体力"
            ]
        },
        {
            "name": "领取体力",
            "entry": "领取体力"
        },
        {
            "name": "领取邮件",
            "entry": "邮件"
        },
        {
            "name": "购买碎片",
            "entry": "购买碎片"
        },
        {
            "name": "宿舍委托",
            "entry": "宿舍委托"
        },
        {
            "name": "宿舍任务",
            "entry": "宿舍任务"
        },
        {
            "name": "拟战场域",
            "entry": "拟战场域",
            "option": [
                "使用临期体力"
            ]
        },
        {
            "name": "守护者行动",
            "entry": "维系者行动"
        },
        {
            "name": "领取任务",
            "entry": "任务"
        },
        {
            "name": "战令",
            "entry": "通行证"
        },
        {
            "name": "结束游戏",
